<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lenovo.prevencion.Metodos">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="0dp"
        android:layout_height="76dp"
        android:layout_marginEnd="49dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="16dp"
        android:fontFamily="monospace"
        android:text='"METODOS ANTICONCEPTIVOS"'
        android:textColor="@color/colorPrimaryDark"
        android:textSize="24sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/tv10"
        android:layout_width="0dp"
        android:layout_height="129dp"
        android:text="TextView"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="77dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="9dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        android:weightSum="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.447"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        android:id="@+id/linearLayout">

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="90dp"
            android:layout_height="96dp"
            android:onClick="lanza3"
            android:src="@drawable/pastillas"
            app:srcCompat="@drawable/pastillas" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="90dp"
            android:layout_height="95dp"
            android:onClick="lanza2"
            android:src="@drawable/condon"
            app:srcCompat="@drawable/condon" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="90dp"
            android:layout_height="95dp"
            android:onClick="lanza4"
            android:src="@drawable/parchee"
            app:srcCompat="@drawable/parchee" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="91dp"
            android:layout_height="102dp"
            android:onClick="lanza5"
            app:srcCompat="@drawable/diu" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="167dp"
        android:layout_marginTop="83dp"
        android:orientation="horizontal"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintTop_toTopOf="@+id/linearLayout"
        android:weightSum="1">

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="102dp"
            android:layout_height="match_parent"
            android:onClick="lanza6"
            android:src="@drawable/anill"
            app:srcCompat="@drawable/anill" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="88dp"
            android:layout_height="match_parent"
            android:onClick="lanza7"
            android:src="@drawable/capu"
            app:srcCompat="@drawable/capu" />

        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="88dp"
            android:layout_height="match_parent"
            android:onClick="lanza8"
            android:src="@drawable/diafra"
            app:srcCompat="@drawable/diafra" />

        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:onClick="lanza9"
            android:src="@drawable/inyeanti"
            app:srcCompat="@drawable/inyeanti" />
    </LinearLayout>

</android.support.constraint.ConstraintLayout>
